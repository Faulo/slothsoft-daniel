<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:template match="/data">		<xsl:variable name="domain" select="*[@data-cms-name='/core/sites']/domain[@active]"/>		<xsl:variable name="pageList" select="$domain | $domain//page[@status-active][@status-public][@ref]"/>		<xsl:variable name="row1" select="$pageList[count(ancestor::*) &lt;= 3]"/>		<xsl:variable name="row2" select="($pageList[../@current] | $pageList[../page/@current])[count(ancestor::*) = 4]"/>				<xsl:if test="count($row1)">			<div class="navi">				<xsl:apply-templates select="$row1"/>			</div>		</xsl:if>		<xsl:if test="count($row2)">			<div class="navi">				<xsl:apply-templates select="$row2"/>			</div>		</xsl:if>		<!--		<xsl:if test="$domain/page[descendant-or-self::*[@current]][page]">			<div class="navi">				<ul>					<xsl:apply-templates select="$domain/page[descendant-or-self::*[@current]]/page"/>				</ul>			</div>		</xsl:if>		<xsl:if test="$domain/page/page[descendant-or-self::*[@current]][page]">			<div class="navi imageGalleryList">				<ul>					<xsl:apply-templates select="$domain/page/page[descendant-or-self::*[@current]]/page">						<xsl:with-param name="createImage" select="true()"/>					</xsl:apply-templates>				</ul>			</div>		</xsl:if>		-->	</xsl:template>		<xsl:template match="domain | page">		<xsl:param name="createImage" select="false()"/>		<xsl:variable name="childPages" select="page[@status-active][@status-public or .//page[@current]]"/>		<a href="{@uri}" data-dict="">			<xsl:if test="self::page or @current">				<xsl:attribute name="class">					<xsl:if test="$childPages and (descendant-or-self::*[@current])">						<xsl:text>open</xsl:text>					</xsl:if>					<xsl:if test="$childPages and not(descendant-or-self::*[@current])">						<xsl:text>close</xsl:text>					</xsl:if>					<xsl:if test="@ext">						<xsl:text>extern</xsl:text>					</xsl:if>					<xsl:if test="@current">						<xsl:text> current</xsl:text>					</xsl:if>				</xsl:attribute>			</xsl:if>			<xsl:value-of select="@title"/>		</a>	</xsl:template></xsl:stylesheet>