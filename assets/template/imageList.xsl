<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"> 	<xsl:template match="/data">		<xsl:variable name="pics" select="*[@data-cms-name='images']"/>		<!--		<xsl:variable name="icons" select="*[@data-cms-name='icons']"/>		<xsl:variable name="disk" select="$icons[@path = 'disk.small.png']"/>		-->		<div>			<dl>				<xsl:for-each select="$pics">					<xsl:sort select="@uri"/>					<dt>						<h1>							<xsl:value-of select="@path"/><br/>							<input value="{@uri}" size="64"/>						</h1>					</dt>					<dd>						<a href="{@uri}" title="{@path}">							<img src="{@thumbnail}" alt="{@path}" />							<div/>						</a>						<samp>							<xsl:value-of select="ceiling(@size div 1024 div 1024)"/>							<xsl:text> MB </xsl:text>							<a href="{@uri}?download" title="Download!" class="downloadFile" download="{@path}">								<!--<img src="data:{$disk/@type};base64,{$disk/@base64}" alt="Download"/>-->								<xsl:text>ðŸ’¾</xsl:text>							</a>							<xsl:text> </xsl:text>							<xsl:value-of select="@width"/>							<xsl:text> x </xsl:text>							<xsl:value-of select="@height"/>							<xsl:text> Pixel</xsl:text>						</samp>					</dd>				</xsl:for-each>			</dl>		</div>	</xsl:template></xsl:stylesheet>